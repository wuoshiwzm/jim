package testjava;


public class Test {
private int[] stack ;
private int top;
	
	public Test(){
		stack = new int[1000];
		top =-1;
	}
	
	public void push(int a){
		top++;
		stack[top] = a;
	}
	
	public static void main(String[] args) {
		
		for(int i =0;i++;i<stack.length){
			stack = new int[1000];
			top =-1;
		}
	}
	
 
}
