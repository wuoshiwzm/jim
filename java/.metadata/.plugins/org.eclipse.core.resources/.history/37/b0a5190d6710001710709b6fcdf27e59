package testjava;

import java.awt.Shape;

public class helloworld {
	
public static void main(String[] args) {
		
	long start=System.currentTimeMillis();   //获取开始时间  

	int k=0;
	int N = 0;
	N = 500;
	k = N/2;
	
	
	long end=System.currentTimeMillis(); //获取结束时间  
	System.out.println("程序运行时间： "+(end-start)+"ms"); 
	}	
}
